    <!-- Upper Right Toolbar: Help and Login Buttons -->
    <div class="toolbar up-right">
        <span class="dropdown pull-right">
            <a id="help-button" data-toggle="dropdown" class="dropdown-toggle" href="#"><%= image_tag "Help.png", size:"34" %></a>
