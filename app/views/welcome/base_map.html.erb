            <div class="modal-content">
                <div class="modal-header">
                    <%= image_tag "LogInWindow-03.png", size:"39" %>
                    <span class="modal-title" id="loginModalLabel">Log in</span>
                </div>
                <div class="modal-body teal">
                    <div class="table-container">
                        <table>
                            <!-- Input Username -->
                            <tr class="text-input">
                                <td class="input-label">
                                    <div> Username </div>
                                </td>
                                <td>
                                    <input type="text" id="input-username" class="input-text form-control inline"/>
                                    <%= image_tag "LogInWindow-01.png", size:"22" %>
                                </td>
                            </tr>
                            <!-- Input Password --> 
                            <tr class="text-input">
                                <td class="input-label">
                                    <div> Password </div>
                                </td>
                                <td>
                                    <input type="text" id="input-password" class="input-text form-control inline"/>
                                </td>
                            </tr>
                            <tr class="footer">
                                <td></td>
                                <td>
                                    <%= image_tag "login-Normal-04.png", size:"93x28" %>
                                    <%= image_tag "login-Normal-05.png", size:"93x28" %>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
