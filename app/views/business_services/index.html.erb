<h1>Listing business_services</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>Service</th>
      <th>Active</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @business_services.each do |business_service| %>
      <tr>
        <td><%= business_service.business_id %></td>
        <td><%= business_service.service_id %></td>
        <td><%= business_service.active %></td>
        <td><%= link_to 'Show', business_service %></td>
        <td><%= link_to 'Edit', edit_business_service_path(business_service) %></td>
        <td><%= link_to 'Destroy', business_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

